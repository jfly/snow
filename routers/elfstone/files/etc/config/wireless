config wifi-device 'radio0'
	option type 'mac80211'
	option path '1e140000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option channel '1'
	option band '2g'
	option htmode 'HT20'
	option cell_density '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option path '1e140000.pcie/pci0000:00/0000:00:01.0/0000:02:00.0'
	option channel '36'
	option band '5g'
	option htmode 'VHT80'
	option cell_density '0'

config wifi-iface '2g_lan'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Hen Wen'
	option encryption 'psk2'
	option key '@wifi_password@'
	option ieee80211r '1'
	option mobility_domain 'beef'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'

config wifi-iface '5g_lan'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'Hen Wen'
	option encryption 'psk2'
	option key '@wifi_password@'
	option ieee80211r '1'
	option mobility_domain 'beef'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'

config wifi-iface '2g_iot'
	option device 'radio0'
	option mode 'ap'
	option network 'iot'
	option ssid 'coll'
	option encryption 'psk2'
	option key '@wifi_iot_password@'

config wifi-iface '5g_iot'
	option device 'radio1'
	option mode 'ap'
	option network 'iot'
	option ssid 'coll'
	option encryption 'psk2'
	option key '@wifi_iot_password@'

config wifi-iface '2g_guest'
	option device 'radio0'
	option mode 'ap'
	option ssid 'Hen Wen Guest'
	option encryption 'psk2'
	option key '@wifi_guest_password@'
	option ieee80211r '1'
	option mobility_domain 'beef'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option network 'guest'

config wifi-iface '5g_guest'
	option device 'radio1'
	option mode 'ap'
	option ssid 'Hen Wen Guest'
	option encryption 'psk2'
	option key '@wifi_guest_password@'
	option ieee80211r '1'
	option mobility_domain 'beef'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option network 'guest'
